{
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
        "es6"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "pretty": true,
    "removeComments": false,
    "sourceMap": false,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es6"
  }
}